{
  "name": "tunnel-cast",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "url": "https://github.com/OfirTheOne/tunnel-cast",
    "type": "GitHub"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "npm tu build:dev",
    "build:dev": "tsc -p tsconfig.json",
    "build:prod": "tsc -p tsconfig.prod.json",
    "start": "ts-node ./src/index.ts",
    "pack-script": "sh pack-publish-version-test.job.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "author": "Ofir G.",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/config": "0.0.34",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^26.0.14",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.10.1",
    "chai": "^4.2.0",
    "config": "^3.2.3",
    "dotenv": "^8.1.0",
    "jest": "^26.5.2",
    "ts-essentials": "^6.0.5",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "utility-types": "^3.10.0"
  },
  "files": [
    "/**"
  ],
  "keywords": [
    "object-to-model",
    "cast",
    "casting",
    "validation",
    "parsing",
    "model",
    "typescript",
    "decorators"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
